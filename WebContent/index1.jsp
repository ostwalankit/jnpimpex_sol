	<script type="text/javascript">
		$.ajaxSetup({
			cache : false
		});

		$(document)
				.ready(
						function() {
							$('#send')
									.click(
											function() {

												var form = $('#contactForm');

												var x = true;

												if (document
														.getElementById("name").value == "") {
													alert("Please enter your Name in the form");
													document.getElementById(
															"name").focus();
													x = false;
													return x;
												}
												if (email.value == "") {
													alert("Email address field cannot be left blank");
													email.focus();
													x = false;
													return x;
												}

												if (email.value != "") {
													var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
													if (reg.test(email.value) == false) {
														alert("Please enter a valid Email address");
														email.focus();
														x = false;
														return x;
													}
												}
												if (phone.value == "") {
													alert("Please enter an phone number");
													phone.focus();
													x = false;
													return x;
												}

												if (phone.value != "") {
													var reg = /^\d{10}$/;

													if (reg.test(phone.value) == false) {
														alert("Please enter a valid phone no.Phone number should be of exactly 10 digits.");
														phone.focus();
														x = false;
														return x;
													}
												}

												if (message.value == "") {
													alert("Message field cannot be left blank. Please enter message.");
													message.focus();
													x = false;
													return x;
												}

												if (x) {
													$('#send').attr("disabled",
															true);

													$('#loadingmessage').show();

													$
															.ajax({
																url : 'sendEmail',
																data : form
																		.serialize(),
																type : 'post',
																success : function(
																		data) {
																	var result = data;
																	//alert(result);
																	$(
																			'#success')
																			.text(
																					result);
																	$(
																			'#loadingmessage')
																			.hide();
																	$('#send')
																			.attr(
																					"disabled",
																					false);
																	$('#name')
																			.val(
																					'')
																	$('#email')
																			.val(
																					'');
																	$('#phone')
																			.val(
																					'');
																	$(
																			'#message')
																			.val(
																					'');
																}

															});
												}
											});
							//return false;
							$('#send').attr("disabled", false);
							$('#loadingmessage').hide();
						});
	</script>
	
